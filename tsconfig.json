{
  "compilerOptions": {
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "target": "es5",
    "outDir": "out",
    "declaration": true,
  },
  "exclude": ["node_modules", "out"],
  "compileOnSave": true
}